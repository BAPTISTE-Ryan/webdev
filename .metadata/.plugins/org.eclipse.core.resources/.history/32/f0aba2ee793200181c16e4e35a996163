package hello;

public class Notes {

	public static void main(String[] args) {
		int[] notes = {18,20,2,14,7,10};
		//moyenne
		int somme=0;
		for (int i = 0; i < notes.length; i++) {
			somme=somme+notes[i];
					}
		double moyenne=1.0*somme/notes.length;
		System.out.println(somme);
		//minimum
		//maximum
		

	}

}
