package hello;

import java.util.Comparator;

public class SongsByTitleComparator implements Comparator<Song>{

	@Override
	public int compare(Song s1, Song s2) {
		if(          s1.getTitle()>s2.getTitle()        )
			{return 1;}
		else if(s1.getTitle()<s2.getTitle())
			{return -1;}
		else {return 0;}
	}
	

}