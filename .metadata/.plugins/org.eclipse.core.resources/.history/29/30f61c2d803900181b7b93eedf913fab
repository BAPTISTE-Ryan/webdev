package hello;

import java.util.List;

public class MusicMain {

	public static void main(String[] args) {

		Personne Drake("Drake","Graham", 31) ;
		Personne Beyonce("Beyonce","knowles",43) ;		
		Personne Truthseekah("Truth","Seekah",33) ;
		Song song1 = new Song("title: Gods Plan",Drake, "genre: HIPOHOP", 360);
		
		Song song2 = new Song("title: Halo",Beyonce, "genre: RNB",720);

		Song song3 = new Song("title: Seven Stars ",Truthseekah, "genre: Underground", 180);
		
		PlayList playList = new PlayList();
		playList.add(song1);
		playList.add(song2);
		playList.add(song3);
		
		
		List<Song> songOrderedByTitle = playList.songsByTitle();
		List<Song> songOrderedByGenre = playList.songsByGenre();
		List<Song> songOrderedByDuration = playList.songsByDuration();
		List<Song> songOrderedByAuthorName = playList.songsByAuthorName();
		
		// "bonjour".compareTo("le monde")
		
		
		
		
		
		
		
		
		

	}

}
